msgid "GNOME Files"
msgstr "GNOME Files"

#: introduction.html:165
msgid "GNOME Software"
msgstr "GNOME Software"

#: introduction.html:169
msgid "File Manager"
msgstr "Datei Manager"

#: introduction.html:170
msgid "Web Browser"
msgstr "Web Browser"

#: introduction.html:171
msgid "Office Suite"
msgstr "Office Suite"

#: introduction.html:172
msgid "Software Store"
msgstr "Software Laden"

