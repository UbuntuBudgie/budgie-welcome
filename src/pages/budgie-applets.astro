<script>
   document.querySelectorAll('.group').forEach(group => {
       const installButton = group.querySelector('.installButton');
       const spinner = group.querySelector('.spinner');
       const removeButton = group.querySelector('.removeButton');

       if (installButton && spinner && removeButton) {
           installButton.addEventListener('click', function() {
               installButton.classList.add('hidden');
               spinner.classList.remove('hidden');
               setTimeout(() => {
                   spinner.classList.add('hidden');
                   removeButton.classList.remove('hidden');
               }, 2000); // Simulate time for applying changes
           });

           removeButton.addEventListener('click', function() {
               removeButton.classList.add('hidden');
               installButton.classList.remove('hidden');
           });
       }
   });
</script>


